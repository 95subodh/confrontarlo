<%= simple_form_for @user do |f| %>
    <%= f.error_notification %>
    <%= f.input :name, placeholder: 'Name' %>
    <%= f.input :location , placeholder: 'Location'%>
    <%= f.input :email, placeholder: 'Email'%>
    <%= f.input :company_name, placeholder: 'Company Name' %>
    <%= f.button :submit %>
<%end%>

