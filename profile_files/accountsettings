  function shww(txt,id){
	
	$('.'+id).find('.frmhd').html(txt).fadeIn("slow");
	$('.'+id).find('.frmhd').css("font-size","19px");
	$('.'+id).find('.frmhd').css("font-weight","normal");
	$('.'+id).find('.frmhd').css("line-height","19px");    

    }


