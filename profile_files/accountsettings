function hideWin(){
	$('.cat-item:visible').hide();
	$('.acount-item:visible').hide();
	$('.substitute').hide();
	$('.downt:visible').css({'border-color':'#fff transparent transparent'});
}
$('.btnn').on('click',function(){
	setCoor("signup");
});
$('.substitute').on('click',function(){
	hideWin();
});
$('.downt,.lgic,.btk,.tpic').on('click',function() {
	$('.substitute').show();
	var x = $(this).parent().find('.guy').offset();
	$('.cat-item:visible').hide();
	$('.acount-item').slideToggle(50);
	$('.acount-item .tip').offset({left:x.left+5});
	$('.downt').css({'border-color':'transparent'});
});
$('.vispart').on('click',function() {
	$('.substitute').slideDown();
	$('.acount-item:visible').hide();
	var x = $(this).offset();
	$('.cat-item').css({left:(x.left-10)+'px',top:(x.top+46)+'px'}).slideToggle();
	$('.cat-item .tip').offset({left:(x.left+5)});
});
$('#qa-np-nc-marker').on('click',function(){
		var x = $(this).offset();
		if ($(this).is('.qa-np-nc-marker-inactive')) {
			$(this).removeClass("qa-np-nc-marker-inactive").addClass("qa-np-nc-marker-active");
			$('#qa-np-nc-window').offset({top:(x.top+14)}).show();
			$('#qa-np-nc-window-tip').offset({left:x.left});
		}else{
			$('#qa-np-nc-window').hide();
			$(this).removeClass('qa-np-nc-marker-active');
			$(this).addClass('qa-np-nc-marker-inactive');

		};
		$('.notibac').width($(document).width()).height($(document).width()).toggle();
	});
$('.qa-np-nc-close-link').on('click',function(){

			$('#qa-np-nc-window').hide();
			$('#qa-np-nc-marker').removeClass('qa-np-nc-marker-active');
		$('#qa-np-nc-marker').addClass('qa-np-nc-marker-inactive');

});

	$('.qa-q-list-item').on('click',function() {
	var c = $(this).find('.lnn').attr("href");
	window.location.href = c;
});
	$('.notibac').click(function(e) {
		$('#qa-np-nc-marker').click();
	});
