    $(document).ready(function() {
     
		$('#advice_widget_category option:contains('+ccc+')').prop('selected', true);
		function urldecode(str) {
  		 return decodeURIComponent((str+'').replace(/\+/g, '%20'));
		}
	    var advice_widgetdata_retrieve=urldecode(getCookie("advice_widgetdata"));
	   	var advice_widgetdata_crossed=getCookie("advice_widgetdatacrossed");
	   	loadSerializedData("Advice_widget", advice_widgetdata_retrieve);

	   	if (advice_widgetdata_crossed != null) {
	            $("#Advice_Platform_Widget").slideUp().delay(1000);
			    $("#Qwidget_icon").delay(2000).slideDown();	   		
	   	}else{
	    	if (getCookie("advice_widgetdata")!=null) {
				loadSerializedData("Advice_widget", advice_widgetdata_retrieve);
				$("#Advice_Platform_Widget").slideDown().animate({height: "255px"}, 500);
					           	$(".Qwidget_submit").addClass('notloggedin');
				$(".Qwidget_submit").removeClass('ntopened');  
	            			}else{
				 $("#Advice_Platform_Widget").delay(8000).slideDown();
	            $(".Qwidget_submit").css("margin-top","-29.4%");  
	            $(".Qwidget_submit").css("margin-right","-16px");  


			}
		}
			

			var Qwidgettitlekyup = 0;
		$('#advice_widget_title').on('keyup',function(e){
	   		Qwidgettitlekyup++;
	   		$('#Qwidget_onbordingtext_title').find('.content').show();
	   		$('#Qwidget_onbordingtext_title').find('.check').show();
	   			var Qwidgettitlelength = $(this).val().length;
	   			Qwidgettitlekyup = Qwidgettitlelength;
	   		if (Qwidgettitlekyup > 15) {

	   				$('#Qwidget_onbordingtext_title').find('.content').text("");
	   				$('#Qwidget_onbordingtext_title').find('.check').html("&#10004;");
	   				$('#Qwidget_onbordingtext_title').find('.check').css("color","green");
	   		};
	    // $('.count').text(120-($(this).val().length));
		});
			var Qwidgetdescriptionkyup = 0;
		$('#advice_widget_description').on('keyup',function(e){
	  		Qwidgetdescriptionkyup++;
	   		$('#Qwidget_onboardingtext_description').find('.content').show();
	   		$('#Qwidget_onboardingtext_description').find('.check').show();	   		
	   			var Qwidgetdesclength = $(this).val().length;
	   			Qwidgetdescriptionkyup = Qwidgetdesclength;
	   		if (Qwidgetdescriptionkyup > 20) {
	   		
	   				$('#Qwidget_onboardingtext_description').find('.content').text("");
	   				$('#Qwidget_onboardingtext_description').find('.check').html("&#10004;");
	   				$('#Qwidget_onboardingtext_description').find('.check').css("color","green");
	   		};
	    // $('.count').text(120-($(this).val().length));
		});
		
		
		$("#advice_widget_title").blur(function(){
			var advice_widgetdata = $("#Advice_widget").serialize();
			createCookie('advice_widgetdata',advice_widgetdata,.5);
				var Qwidgettitlelength = $(this).val().length;
				if (Qwidgettitlelength < 1) {
	   				$('#Qwidget_onbordingtext_title').find('.content').text("Required");
	   				$('#Qwidget_onbordingtext_title').find('.check').html("&#33;");
	   				$('#Qwidget_onbordingtext_title').find('.check').css("color","red");
	   			}else if(Qwidgettitlelength >= 1 && Qwidgettitlelength < 15){
	   				$('#Qwidget_onbordingtext_title').find('.content').text("not enough, tell us more.");
	   				$('#Qwidget_onbordingtext_title').find('.check').html("&#10007;");
	   				$('#Qwidget_onbordingtext_title').find('.check').css("color","yellow");

	   			}
	   		$('#Qwidget_onbordingtext_title').find('.content').fadeIn();
	   		$('#Qwidget_onbordingtext_title').find('.check').fadeIn();


		});
		$("#advice_widget_description").blur(function(){
			var advice_widgetdata = $("#Advice_widget").serialize();
			createCookie('advice_widgetdata',advice_widgetdata,.5);
			var Qwidgetdesclength = $(this).val().length;
	   			if (Qwidgetdesclength < 1) {
	   				$('#Qwidget_onboardingtext_description').find('.content').text("Required");
	   				$('#Qwidget_onboardingtext_description').find('.check').html("&#33;");
	   				$('#Qwidget_onboardingtext_description').find('.check').css("color","red");
	   			}else if(Qwidgetdesclength > 1 && Qwidgetdesclength < 20){
	   				$('#Qwidget_onboardingtext_description').find('.content').text("not enough, tell us more.");
	   				$('#Qwidget_onboardingtext_description').find('.check').html("&#10007;");
	   				$('#Qwidget_onboardingtext_description').find('.check').css("color","yellow");

	   			}
	   		$('#Qwidget_onboardingtext_description').fadeIn();	   		

		});			
        $("#Advice_Platform_Widget").click(function(){
        	$("#Advice_Platform_Widget").animate({height: "255PX"}, 500);
	        $(".Qwidget_submit").animate({"margin-top": "1%"}, 500);    
           	$(".Qwidget_submit").addClass('opened');
	        });
	    $("#Qwidget_cross").click(function(){
	  	createCookie('advice_widgetdatacrossed',1,7);

	            $("#Advice_Platform_Widget").slideUp().delay(200);
	            			  $("#Qwidget_icon").delay(9).slideDown();
    
        });
	    $("#Qwidget_icon").click(function(){
	    		  	createCookie('advice_widgetdatacrossed',1,-1);

				$("#Advice_Platform_Widget").slideDown().animate({height: "255px"}, 500);
	            			  $("#Qwidget_icon").slideUp();
    
        });
	   			$('#Advice_widget').submit(function(event){
    				var isValid = true;
				if ($(".Qwidget_submit").hasClass("Qwidgetsubmitloggedin")){
						if (getCookie("advice_widgetdata")!=null) {
														isValid = true;
			    				
							}else{
													isValid = false;

							}
				}else if ($(".Qwidget_submit").hasClass("ntopened")) {
	           		if ($(".Qwidget_submit").hasClass("opened")) {
	           				var isUser=getCookie("UID");
			var advice_widgetdata = $("#Advice_widget").serialize();
			createCookie('advice_widgetdata',advice_widgetdata,.5);
			if (isUser!=null) {
				setCoor("login");
				shww('Almost done. <br /> Login below to post and track your question','login');

			}else{
				setCoor("signup");
				shww('Almost done. <br /> Signup below to post and track your question','signup');

			} 
	           		};
	           	$(".Qwidget_submit").addClass('notloggedin');
				$(".Qwidget_submit").removeClass('ntopened');  
			isValid = false;

				}else  if($(".Qwidget_submit").hasClass("notloggedin")){
				var isUser=getCookie("UID");
			var advice_widgetdata = $("#Advice_widget").serialize();
			createCookie('advice_widgetdata',advice_widgetdata,.5);
			if (isUser!=null) {
				setCoor("login");
				shww('Almost done. <br /> Login below to post and track your question','login');

			}else{
				setCoor("signup");
				shww('Almost done. <br /> Signup below to post and track your question','signup');

			} 

			isValid = false;

				}
					if($(".Qwidget_submit").hasClass("Qwidgetsubmitloggedin")){
							var Qwidgetdesclength = $('#advice_widget_description').val().length;
	   			if (Qwidgetdesclength < 1) {
	   				$('#Qwidget_onboardingtext_description').find('.content').text("Required");
	   				$('#Qwidget_onboardingtext_description').find('.check').html("&#33;");
	   				$('#Qwidget_onboardingtext_description').find('.check').css("color","red");
	   				isValid = false;
	   				 		$('#Qwidget_onboardingtext_description').fadeIn();	
	   			}else if(Qwidgetdesclength > 1 && Qwidgetdesclength < 20){
	   				$('#Qwidget_onboardingtext_description').find('.content').text("not enough, tell us more.");
	   				$('#Qwidget_onboardingtext_description').find('.check').html("&#10007;");
	   				$('#Qwidget_onboardingtext_description').find('.check').css("color","yellow");
	   				isValid = false;
	   				 		$('#Qwidget_onboardingtext_description').fadeIn();	

	   			}
	   		$('#Qwidget_onboardingtext_description').fadeIn();	
	   			   		$('#Qwidget_onbordingtext_description').find('.content').fadeIn();
	   		$('#Qwidget_onbordingtext_description').find('.check').fadeIn();
	   					var Qwidgettitlelength = $('#advice_widget_title').val().length;
				if (Qwidgettitlelength < 1) {
	   				$('#Qwidget_onbordingtext_title').find('.content').text("Required");
	   				$('#Qwidget_onbordingtext_title').find('.check').html("&#33;");
	   				$('#Qwidget_onbordingtext_title').find('.check').css("color","red");
	   					   				isValid = false;
		$('#Qwidget_onbordingtext_title').find('.content').fadeIn();
	   		$('#Qwidget_onbordingtext_title').find('.check').fadeIn();
	   			}else if(Qwidgettitlelength >= 1 && Qwidgettitlelength < 15){
	   				$('#Qwidget_onbordingtext_title').find('.content').text("not enough, tell us more.");
	   				$('#Qwidget_onbordingtext_title').find('.check').html("&#10007;");
	   				$('#Qwidget_onbordingtext_title').find('.check').css("color","yellow");
	   				isValid = false;
	   						$('#Qwidget_onbordingtext_title').find('.content').fadeIn();
	   		$('#Qwidget_onbordingtext_title').find('.check').fadeIn();
	   			}

   		


					}				

				    if (!isValid) {

				        event.preventDefault();
				    }else{
				    	$('#Qwidget_loading').show();
				    }
				});
	   	
			
  });
    function loadSerializedData(formId, data)
		{
			var tmp = data.split('&'), dataObj = {};
			
			// Bust apart the serialized data string into an obj
			for (var i = 0; i < tmp.length; i++)
			{
				var keyValPair = tmp[i].split('=');
				dataObj[keyValPair[0]] = keyValPair[1];
			}

			// Loop thru form and assign each HTML tag the appropriate value
			$('#' + formId + ' :input').each(function(index, element) {
				if (dataObj[$(this).attr('name')])
					$(this).val(dataObj[$(this).attr('name')]);
			});
		}

</script>
