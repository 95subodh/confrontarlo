<script type="text/javascript">
 function checkboxvalidate () {
	var userprefcheck= "1"
		if (userprefcheck == "1") {
		$('#PostQues').attr('checked','checked');
		};
		var userprefcheck= "1"
		if (userprefcheck == "1") {
		$('#PostEm').attr('checked','checked');
		};
		var userprefcheck= "1"
		if (userprefcheck == "1") {
		$('#Postbest').attr('checked','checked');
		};
		var userprefcheck= "1"
		if (userprefcheck == "1") {
		$('#Postvote').attr('checked','checked');
		};    
	}

$('.checkset').change(function() {
	$("#settsub").removeClass('cncl');
});






$("#gensett").submit(function(e) {
 		$('.atajax').show();
    var url = "http://www.findyogi.com/gensett"; // the script where you handle the form input.
 	// alert($("#gensett").serialize());
    $.ajax({
           type: "POST",
           url: url,
           data: $("#gensett").serialize(), // serializes the form's elements.
           success: function(data)
           {	
           	 $('.atajax').hide();
	$("#settsub").addClass('cncl');// show response from the php script.
           }

         });

        e.preventDefault();
});

	function readURL(input) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();            
            reader.onload = function (e) {
                $('#lbll').attr('src', e.target.result);
            }
            reader.readAsDataURL(input.files[0]);
        }
    }
  
	$(function(){
		checkboxvalidate();
		$('#back-top').remove();
		var name = $('#chdNm').val(),
		loc = $('#chdLc').val(),
		dob = $('#chdDB').val(),
		web = $('#chdWB').val();
		page="home";
		xh = "set"
		$('body').css({"background-image":"url('')"});
		$('.chfile').on('click',function(){$('.upld').click();});
		$('#General').show();
		$('.rifp').on('click',function(){setCoor('forget');});
		$("input:file").change(function(e){
			var fn = $(this).val(),
	       	files = e.target.files,
	       	winh = $(window).height();
	       	for (var i = 0, file; file = files[i]; i++) {
			    var name = file.name,
		        extension = name.split('.')[name.split('.').length - 1].toLowerCase(),
		        size = file.size;
				if(size > 4194304){
					shw('bad','file should be less than 4MB.','geneform','rd');
					$('.primmsg').html('max upload size is 4MB.');
					$('.hidsbmt').show();
					$('body,html').animate({
						scrollTop: $('.tabctrl').offset().top
					}, 1000);
				}else{
					$('.warn').hide();
					$('.primmsg').html('');
					$('.hidsbmt').hide();
				}
		  	}
	       $(this).siblings('.chfile').hide().siblings('.hidb').show();
	       readURL(this);
	     });
		$('.cncel').on('click',function(){
			$(this).parents('.hidb').hide();
			$('.chfile').show();
		})
		$('.tab-option').on('click',function(){
			$('.tab-option').removeClass('active');
			$('.tab-option').addClass('tab-color');
			$(this).addClass('active');
			$(this).addClass('tab-color');
			var x = $(this).text();
			x = /\s/.test(x) ? x.split(" ")[0] : x ;
			$('.ptbpg').hide();
			$('#'+x).show();
		});
		$('input#chdPw,input#chdOPw').on('propertychange keyup input paste', function(ee){
			$(this).val() == "" ? $(this).siblings('.shpw').slideUp("slow") : $(this).siblings('.shpw').height($(this).outerHeight()).css({'line-height':$(this).outerHeight()+'px'}).slideDown("slow");
		});
		$('#chdLc').on('input',function(){
			$(this).parents("form").find('input[type="submit"]').removeAttr('disabled');
			$('.hidsbmt').hide();
		})
		var $input;
		$(".shpw").click(function () {
			$input = $(this).siblings('input');
			var change = "";
			if ($(this).html() === "Show Password") {
				$(this).html("Hide Password")
				change = "text";
			} else {
				$(this).html("Show Password");
				change = "password";
			}
			var rep = $("<input type='" + change + "' />")
				.attr("id", $input.attr("id"))
				.attr('class', $input.attr('class'))
				.attr('placeholder',$input.attr('placeholder'))
				.val($input.val())
				.insertBefore($input);
			$input.remove();
			$input = rep;
		}).insertAfter($input);
		$('#chdDB').on('blur', function() {
			if($(this).val!=dob){
				var datval =checkdate($(this));
				datval ? ($(this).parents("form").find('input[type="submit"]').removeAttr('disabled'),$('.hidsbmt').hide(),$('.warn').hide(),$(this).removeClass('first').addClass('riht')) : ($(this).removeClass('riht').addClass('first'),$('.hidsbmt').show(),$(this).parents("form").find('input[type="submit"]').attr('disabled','disabled'));
			}
			if($(this).val()===""||null){$(this).removeClass('first riht');$('.warn').hide();}
			dob = $(this).val();
		});
		$('#chdNm').on('blur', function(){

					if(/^[\w\s]*$/.test($(this).val())){
						$(this).val().length >= 4 ? ($(this).parents("form").find('input[type="submit"]').removeAttr('disabled'),$('.hidsbmt').hide(),$('.warn').hide(),$(this).removeClass('first').addClass('riht')) : ($(this).parents("form").find('input[type="submit"]').attr('disabled','disabled'),$('.hidsbmt').show(),shw('bad','Name should have more than 3 characters.','geneform','rd'),$(this).removeClass('riht').addClass('first'));
					}else{
						$(this).parents("form").find('input[type="submit"]').attr('disabled','disabled');
						$('.hidsbmt').show();
						shw('bad','Name should not have special characters.','geneform','rd');
						$(this).removeClass('riht').addClass('first');
					}
	
						if($(this).val()===""||null){$(this).removeClass('first riht');$('.warn').hide();}
			name = $(this).val();
		});
		$('#chdWB').on('blur', function(){
			if($(this).val()!=web){
				var valurl = validurl($(this).val());
				valurl ? ($('.warn').hide(),$(this).removeClass('first').addClass('riht'),$('.hidsbmt').hide(),$(this).parents("form").find('input[type="submit"]').removeAttr('disabled')) : (shw('bad','Enter a valid Blog/Website Link','geneform','rd'),$(this).removeClass('riht').addClass('first'),$('.hidsbmt').show(),$(this).parents("form").find('input[type="submit"]').attr('disabled','disabled'));
			}
			if($(this).val()===""||null){$(this).removeClass('first riht');$('.warn').hide();}
			web = $(this).val();
		});
		$('#confpassword').keyup(function(){
	        if($(this).val().length >=4 && $(this).val().length >= $('#newpassword').val().length){
	            $('#newpassword').val() === $(this).val() ? ($(this).addClass("riht"),$('.warn').hide(),$('.hidsbmt').hide(),$(this).parents("form").find('input[type="submit"]').removeAttr('disabled')) : ($(this).removeClass('riht').addClass("first"),shw("bad","passwords don't match","rstpw",'rd'),$('.hidsbmt').show(),$(this).parents("form").find('input[type="submit"]').attr('disabled','disabled'));
	        }else{
	        	$(this).removeClass('first riht');
	        	$('.hidsbmt').show();
	        	$(this).parents("form").find('input[type="submit"]').attr('disabled','disabled');
	        }
	    });
	    $('#oldpassword,#newpassword').blur(function(){
	    	if($(this).val().length>0){
		    	if($(this).val().length>=4){
		    		$('.warn').hide();
		    		$(this).removeClass('first').addClass('riht');
		    	}else{
		    		shw('bad','Password must be minimum 4 characters long.','changpass','rd');
		    		$(this).removeClass('riht').addClass('first');
		    	}
		    }else{
		    	$(this).removeClass("riht first");
		    	$('.warn').hide();
		    }
	    });
	});
function checkdate(input){
	var validformat= /^\d{2}[./-]\d{2}[./-]\d{4}$/;
	var returnval=false;
	if (!validformat.test(input.val())){
		shw('bad','Invalid date, fill the date in dd-mm-yyyy format eg. 19-03-1989','geneform','rd');
	}else{
		returnval=true;
		if (returnval==false) 
			input.select();
		returnval = returnval ? checkFuture(input.val()) : false;
	}
	return returnval;
	// var timestamp = Date.parse(str),
	// str = input.val(),date = null,ret = false;
	// isNaN(timestamp) == false ? (date = new Date(timestamp),ret=true) : (shw('bad','Invalid date, fill the date in dd-mm-yyyy format eg. 19-03-1989','geneform','rd'),ret=false);
	// return ret;
}
function checkFuture(bdate){
	var today = new Date(),
    dd = today.getDate(),
    mm = today.getMonth()+1,
    yyyy = today.getFullYear(),flag;
    if(dd<10)
    	dd='0'+dd 
    if(mm<10)
    	mm='0'+mm
    today = dd+'-'+mm+'-'+yyyy;
    flag = new Date(today.split('-').reverse().join('-')) >  new Date(bdate.split('-').reverse().join('-'));
    flag ? null : shw('bad','You seem to have returned from the future. Please put a more Earthly Date of Birth.','geneform','rd');
    return flag;
}
function validurl(url)
{
	return url.match(/^(ht|f)tps?:\/\/[a-z0-9-\.]+\.[a-z]{2,4}\/?([^\s<>\#%"\,\{\}\\|\\\^\[\]`]+)?$/);
}
